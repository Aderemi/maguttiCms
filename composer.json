{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "artesaos/seotools": "^0.9.2",

        "cocur/slugify": "^2.5",
        "dimsav/laravel-translatable": "^6.0",
        "guzzlehttp/guzzle": "~6.0",
        "hisorange/browser-detect": "2.*",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "laravelcollective/html": "5.5.1",

        "mcamara/laravel-localization": "1.2.*",
        "predis/predis": "~1.0",

        "vinkla/shield": "^2.1",
        "zizaco/entrust": "5.2.x-dev",
        "maatwebsite/excel": "~2.1.0"
    },

    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },

    "autoload": {
        "classmap": [
            "database",
            "app/maguttiCms",
            "app/maguttiCms/Sluggable"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
			"app/maguttiCms/Tools/helpers.php",
            "app/maguttiCms/Tools/ma_helper.php",
            "app/maguttiCms/Tools/Stringable.class.php"
        ]
    },

    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },

    "prefer-stable": true
}
