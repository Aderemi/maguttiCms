{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "artesaos/seotools": "^0.10.0",
    "barryvdh/laravel-cors": "^0.11.2",
    "cocur/slugify": "^2.5",
    "dimsav/laravel-translatable": "^9.2",
    "fideloper/proxy": "~4.0",
    "guzzlehttp/guzzle": "~6.0",
    "hisorange/browser-detect": "2.*",
    "intervention/image": "^2.3",
    "intervention/imagecache": "^2.3",
    "laravel/framework": "5.7.*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "5.7.*",
    "maatwebsite/excel": "^3.0",
    "magutti/magutti-builders": "dev-master",
    "mcamara/laravel-localization": "1.2.*",
    "pbmedia/laravel-ffmpeg": "^3.0",
    "predis/predis": "~1.0",
    "spatie/laravel-fractal": "^5.2",
    "spatie/laravel-sitemap": "^5.2",
    "srmklive/paypal": "=1.6.1",
    "tymon/jwt-auth": "^0.5.10",
    "vinkla/shield": "^2.3",
    "zizaco/entrust": "5.2.x-dev"
  },

  "require-dev": {
    "filp/whoops": "~2.0",
    "nunomaduro/collision": "~1.1",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "beyondcode/laravel-dump-server": "^1.0",
    "symfony/thanks": "^1.0",
    "barryvdh/laravel-debugbar": "^3.0"
  },

  "autoload": {
    "classmap": [
		  "database/seeds",
		  "database/factories",
      "app/maguttiCms",
      "app/maguttiCms/Sluggable"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/maguttiCms/Tools/helpers.php",
      "app/maguttiCms/Tools/ma_helper.php",
      "app/maguttiCms/Tools/Stringable.class.php",
      "app/maguttiCms/Tools/Enums.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
